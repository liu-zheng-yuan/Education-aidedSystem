<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nju.dao.TeacherDao">
    <select id="getTeacher4Login" resultType="com.nju.bean.Teacher">
        select * from teacher where loginAcct = #{loginAcct} and passWord = #{passWord};
    </select>


</mapper>